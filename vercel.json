{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "public": false,
  "functions": {
    "dist/index.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/dist/index.js"
    }
  ]
}
